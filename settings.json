{
    "editor.fontFamily": "FiraCode Nerd Font",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.minimap.enabled": false,
    "editor.tabCompletion": "on",
    "git.autofetch": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.autoComplete.addBrackets": true,
    "python.defaultInterpreterPath": "$(brew --prefix)/Caskroom/mambaforge/base/envs/${workspaceFolderBasename}/bin/python",
    "python.languageServer": "Pylance",
    "r.alwaysUseActiveTerminal": true,
    "r.bracketedPaste": true,
    "r.rterm.mac": "$(brew --prefix)/Caskroom/mambaforge/base/envs/${workspaceFolderBasename}/bin/radian",
    "r.sessionWatcher": true,
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.macOptionIsMeta": true,
    "window.zoomLevel": 0,
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "$HOME/.config/nvim/init.vim",
    "tabnine.experimentalAutoImports": true,
    "extensions.ignoreRecommendations": false,
    "jupyter.alwaysTrustNotebooks": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindowMode": "perFile",
    "jupyter.sendSelectionToInteractiveWindow": true,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ]
}
